B_Hight_Purple='\033[1;95m'     # Purple
B_Hight_Cyan='\033[1;96m'       # Cyan
B_Hight_White='\033[1;97m'      # White

# High Intensity backgrounds
On_IBlack='\033[0;100m'   # Black
